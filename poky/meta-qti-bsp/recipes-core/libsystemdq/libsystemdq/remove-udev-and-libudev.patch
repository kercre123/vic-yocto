-        if (strlen(guid_str) < guid_str_len || guid_str[0] != '{' || guid_str[guid_str_len-1] != '}')
-                return NULL;
-
-        for (i = 1, k = 0; i < guid_str_len-1; i++) {
-                if (guid_str[i] == '-')
