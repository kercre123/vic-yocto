#
# Default properties to be set for apq8053
#

ro.product.board=msm8953

vendor.audio.hw.aac.encoder=true

#Audio voice concurrency related flags
vendor.voice.playback.conc.disabled=true
vendor.voice.record.conc.disabled=false
vendor.voice.voip.conc.disabled=true

ro.vendor.audio.sdk.ssr=false

##fluencetype can be "fluence" or "fluencepro" or "none"
ro.vendor.audio.sdk.fluencetype=fluencepro
persist.vendor.audio.fluence.voicecall=false
persist.vendor.audio.fluence.voicerec=false
persist.vendor.audio.fluence.speaker=false
persist.vendor.audio.fluence.audiorec=true

vendor.audio.tunnel.encode = false
#Buffer size in kbytes for compress offload playback
vendor.audio.offload.buffer.size.kb=64

#Minimum duration for offload playback in secs
audio.offload.min.duration.secs=30
#Enable offload audio video playback by default
audio.offload.video=true

#Enable audio track offload by default
vendor.audio.offload.track.enable=true

#enable downsampling for multi-channel content > 48Khz
vendor.audio.playback.mch.downsample=true

#enable software decoders for ALAC and APE.
vendor.audio.use.sw.alac.decoder=true
vendor.audio.use.sw.ape.decoder=true

#enable dsp gapless mode by default
vendor.audio.offload.gapless.enabled=true

#multi offload
vendor.audio.offload.multiple.enabled=false

#multiple compress record
vendor.audio.apptype.multirec.enabled=true
vendor.audio.record.multiple.enabled=true

#32bit record
vendor.audio.capture.pcm.32bit.enable=false

#enable pbe effects
vendor.audio.safx.pbe.enabled=true

#property for AudioSphere Post processing
vendor.audio.pp.asphere.enabled=false

#property to enable DS2 dap
vendor.audio.dolby.ds2.enabled=true

#Offset for audio timestamp
persist.qmmf.timestamp.adjust=-150000

#isp clock optimization enable(1)/disable(0)
persist.camera.isp.clock.optmz=0

#property to enable audio HAL server
persist.vendor.audio.qas.enabled=false
